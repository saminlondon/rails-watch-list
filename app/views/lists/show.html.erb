<h2><%= @list.name %></h2>

<% @list.bookmarks.each do |b| %>
<%= b.movie.title %>
<%= b.comment %>
<% end %>


<%= link_to "back to list", lists_path, class: "btn btn-light" %>

<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.input :comment %>
  <%= f.association :movie %>
  <%= f.button :submit %>
<% end %>
